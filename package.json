{
  "name": "slumber-numbers",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:ci": "ng build --prod --progress=false",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --webdriver-update",
    "lint": "ng lint",
    "ng": "ng",
    "pa11y": "pa11y-ci --sitemap http://slumbernumbers.com/sitemap.xml --sitemap-find slumbernumbers.com --sitemap-replace localhost:4200 --sitemap-find https --sitemap-replace http",
    "pa11y:ci": "yarn start & yarn wait && yarn pa11y",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "yarn build:ci && yarn test:headless && cat coverage/lcov.info | yarn coveralls",
    "test:headless": "ng test --watch=false --browsers ChromeHeadless --code-coverage --progress=false",
    "wait": "wait-on http-get://localhost:4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^11.0.5",
    "@angular/fire": "^5.1.0",
    "@angular/forms": "^9.1.13",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@firebase/app-types": "^0.3.2",
    "core-js": "^2.5.4",
    "firebase": "^5.5.7",
    "hammerjs": "^2.0.8",
    "ngx-take-until-destroy": "^5.2.3",
    "rxjs": "^6.6.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.14",
    "@angular/cli": "~9.1.14",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "coveralls": "^3.0.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "pa11y-ci": "^2.1.1",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3",
    "wait-on": "^3.2.0"
  }
}